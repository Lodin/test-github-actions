{
  "extends": [
    "eslint-config-poetez",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "rules": {
    "callback-return": "off",
    "func-names": "off",
    "max-classes-per-file": "off",
    "no-unused-expressions": "off",
    "prefer-rest-params": "off"
  }
}
